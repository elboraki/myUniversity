 <header>
     <!-- Dropdown Structure -->
    <ul id="dropdown1" class="dropdown-content">
          <% if logged_in? %>
          <li><%= link_to 'logout', logout_path ,method: :delete %></li>
        <% else %>
          <li><%= link_to 'login', login_path %></li>
           <li class="divider"></li>
        <li><%= link_to 'Sign_up', new_student_path %></li>
        <% end %>
       
    </ul>
    <ul id="dropdown2" class="dropdown-content">
        <% if logged_in? %>
          <li><%= link_to 'logout', logout_path %></li>
        <% else %>
          <%= link_to 'login', login_path %>
        <% end %>
        <li class="divider"></li>
        <li><a href="#!">three</a></li>
    </ul>
    <nav>
        <div class="container">
            <div class="nav-wrapper">
                <a href="#" class="brand-logo right">myUniversity</a>
                <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
                <ul id="nav-mobile" class="left hide-on-med-and-down">
                    <li><a href="#">Courses</a></li>
                    <li><a href="#">Enrollement</a></li>
                    <li><a class="dropdown-trigger" href="#!" data-target="dropdown1">Account<i class="material-icons right">arrow_drop_down</i></a></li>
                </ul>
            </div>
        </div>
    </nav>

    <ul class="sidenav" id="mobile-demo">
        <li><a href="#">Courses</a></li>
        <li><a href="#">Enrollement</a></li>
        <li><a class="dropdown-trigger" href="#!" data-target="dropdown2">Account<i class="material-icons right">arrow_drop_down</i></a></li>
    </ul>
    <div class="container">
        <div class="row center sign-up-form">
            <div class="col s12 m12 l12">
                <% flash.each do |key,msg| %>
                <div class="card-panel blue lighten-2 left-align" id="fade-out-target">
                    <span class="white-text">
                        
                            <ul>
                                <li><%= msg %></li>
                            </ul>
                      
                    </span>
                </div>
                <% end %>
            </div>
        </div>
    </div>
 </header>
